{
  "expo": {
    "name": "AlertH2O",
    "slug": "AlertH2O",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "BLUETOOTH",
          "BLUETOOTH_ADMIN",
          "ACCESS_FINE_LOCATION",
          "BLUETOOTH_CONNECT",
          "BLUETOOTH_SCAN"
        ]
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}